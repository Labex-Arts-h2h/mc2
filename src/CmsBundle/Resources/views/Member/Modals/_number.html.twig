
{% if number.completeTitle == 2 or number.completeTc == 2 or number.completeStructure == 2 or number.completeShots == 2 or number.completePerformance == 2 or number.completeBackstage == 2 or number.completeTheme == 2 or number.completeMood == 2 or number.completeMusic == 2 or number.completeReference  == 2 %}
<li class="collection-item avatar film{{ number.film.filmId }} number-item number-assist">
    <i class="material-icons circle orange">movie</i>
{% else %}

    {% if (number.completeTitle and number.completeTc and number.completeStructure and number.completeShots and number.completePerformance and number.completeBackstage and number.completeTheme and number.completeMood and number.completeMusic and number.completeReference )  == 1 %}
