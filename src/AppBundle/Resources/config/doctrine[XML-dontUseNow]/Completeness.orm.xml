<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Completeness" table="completeness">
    <id name="completenessId" type="integer" column="completeness_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Complete/ incomplete/ partial reprise / pause / â€¦</option>
      </options>
    </field>
    <many-to-many field="number" target-entity="Number" inversed-by="completeness" fetch="LAZY">
      <join-table name="completeness_has_number">
        <join-columns>
          <join-column name="completeness_id" referenced-column-name="completeness_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
