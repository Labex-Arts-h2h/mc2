<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Stagenumber" table="stageNumber">
    <indexes>
      <index name="fk_stageNumber_setting1_idx" columns="setting_id"/>
      <index name="fk_stageNumber_stageShow1_idx" columns="stageShow_id"/>
    </indexes>
    <id name="stagenumberId" type="integer" column="stageNumber_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="groupId" type="integer" column="group_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="order" type="integer" column="order" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Place du num√©ro dans le show</option>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Voir thesaurus</option>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ibdb" type="integer" column="ibdb" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="setting" target-entity="Place" fetch="LAZY">
      <join-columns>
        <join-column name="setting_id" referenced-column-name="place_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="stageshow" target-entity="Stageshow" fetch="LAZY">
      <join-columns>
        <join-column name="stageShow_id" referenced-column-name="stageShow_id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="ensemble" target-entity="Ensemble" mapped-by="stagenumber" fetch="LAZY"/>
    <many-to-many field="number" target-entity="Number" mapped-by="stagenumber" fetch="LAZY"/>
    <many-to-many field="costumes" target-entity="Costumes" mapped-by="stagenumber" fetch="LAZY"/>
    <many-to-many field="musical" target-entity="Musical" mapped-by="stagenumber" fetch="LAZY"/>
    <many-to-many field="dancing" target-entity="Dancing" mapped-by="stagenumber" fetch="LAZY"/>
    <many-to-many field="song" target-entity="Song" mapped-by="stagenumber" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
