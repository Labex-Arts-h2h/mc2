<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ImportComposer" table="import_composer">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="song" type="string" column="song" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="person" type="string" column="person" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="songId" type="integer" column="song_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="personId" type="integer" column="person_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
