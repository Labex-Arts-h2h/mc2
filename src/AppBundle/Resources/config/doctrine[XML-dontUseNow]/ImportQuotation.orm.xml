<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ImportQuotation" table="import_quotation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="film" type="integer" column="film" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="timecode" type="integer" column="timecode" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="external" type="text" column="external" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
