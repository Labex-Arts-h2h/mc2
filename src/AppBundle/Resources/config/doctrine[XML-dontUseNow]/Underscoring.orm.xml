<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Underscoring" table="underscoring">
    <id name="underscoringId" type="integer" column="underscoring_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Reprise d’un thème musical</option>
      </options>
    </field>
    <field name="timecode" type="integer" column="timecode" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="film" target-entity="Film" mapped-by="underscoring" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
