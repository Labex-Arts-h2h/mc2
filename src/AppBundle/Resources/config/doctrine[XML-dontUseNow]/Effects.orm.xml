<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Effects" table="effects">
    <id name="effectsId" type="integer" column="effects_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">

Visual ou sp√©cial</option>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="number" target-entity="Number" mapped-by="effects" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
