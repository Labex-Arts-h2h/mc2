<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Item" table="item">
    <indexes>
      <index name="structure" columns="structure"/>
      <index name="fk_item_Thesaurus" columns="thesaurus_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="structure_2" columns="structure"/>
    </unique-constraints>
    <id name="itemId" type="integer" column="item_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="validationId" type="integer" column="validation_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="thesaurus" target-entity="Thesaurus" fetch="LAZY">
      <join-columns>
        <join-column name="thesaurus_id" referenced-column-name="thesaurus_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="structure" target-entity="Number" fetch="LAZY">
      <join-columns>
        <join-column name="structure" referenced-column-name="number_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
