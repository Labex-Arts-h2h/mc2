<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Film" table="film">
    <id name="filmId" type="integer" column="film_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productionyear" type="integer" column="productionyear" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="released" type="integer" column="released" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idImdb" type="string" column="id_imdb" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="studio" type="string" column="studio" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="distributor" type="string" column="distributor" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="color" type="string" column="color" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Essayer de le faire remonter techniquement via les couleurs des numéros</option>
      </options>
    </field>
    <field name="ratio" type="string" column="ratio" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="length" type="integer" column="length" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contract" type="string" column="contract" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Date d’acquisition
</option>
      </options>
    </field>
    <field name="rights" type="integer" column="rights" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Somme en dollars</option>
      </options>
    </field>
    <field name="negative" type="integer" column="negative" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Negative cost : budget de production en dollar</option>
      </options>
    </field>
    <field name="pna" type="integer" column="pna" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Print and advertising cost, valeur en $
</option>
      </options>
    </field>
    <field name="usRentals" type="integer" column="us_rentals" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Dollars</option>
      </options>
    </field>
    <field name="foreignRentals" type="integer" column="foreign_rentals" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">dollars</option>
      </options>
    </field>
    <field name="totalRentals" type="integer" column="total_rentals" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Worldwide rentals en dollar
</option>
      </options>
    </field>
    <field name="usBoxoffice" type="integer" column="us_boxoffice" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">dollars</option>
      </options>
    </field>
    <field name="foreignBoxoffice" type="integer" column="foreign_boxoffice" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">dollars</option>
      </options>
    </field>
    <field name="totalBoxoffice" type="integer" column="total_boxoffice" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Dollars</option>
      </options>
    </field>
    <field name="sourceEco" type="text" column="source_eco" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="archives" type="text" column="archives" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Archive et commentaires</option>
      </options>
    </field>
    <field name="deleted" type="text" column="deleted" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Indication des numéros supprimés
</option>
      </options>
    </field>
    <field name="adaptation" type="string" column="adaptation" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Adaptation de Broadway, voir thésaurus?
</option>
      </options>
    </field>
    <field name="remake" type="string" column="remake" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Yes/ No
</option>
      </options>
    </field>
    <field name="song" type="integer" column="song" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numberMedley" type="integer" column="number_medley" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numberCompletenessIdcompleteness" type="integer" column="number_completeness_idcompleteness" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numberEnsembleMusic" type="integer" column="number_ensemble_music" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numberEnsembleDance" type="integer" column="number_ensemble_dance" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="verdict" type="string" column="verdict" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">PCA verdict (unacceptable/ acceptable)</option>
      </options>
    </field>
    <field name="pcaTexte" type="text" column="pca_texte" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="legion" type="string" column="legion" length="5" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">A, B, C ou D</option>
      </options>
    </field>
    <field name="protestant" type="string" column="protestant" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="harrisson" type="string" column="harrisson" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bord" type="string" column="bord" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="underscoring" target-entity="Underscoring" inversed-by="film" fetch="LAZY">
      <join-table name="film_has_underscoring">
        <join-columns>
          <join-column name="film_id" referenced-column-name="film_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="underscoring_id" referenced-column-name="underscoring_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="state" target-entity="State" inversed-by="film" fetch="LAZY">
      <join-table name="film_has_state">
        <join-columns>
          <join-column name="film_id" referenced-column-name="film_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="state_id" referenced-column-name="state_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="censorship" target-entity="Censorship" inversed-by="film" fetch="LAZY">
      <join-table name="film_has_censorship">
        <join-columns>
          <join-column name="film_id" referenced-column-name="film_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="censorship_id" referenced-column-name="censorship_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
