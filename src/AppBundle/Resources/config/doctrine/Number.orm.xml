<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Number" table="number">
    <indexes>
      <index name="fk_number_film1_idx" columns="film_id"/>
    </indexes>
    <id name="numberId" type="integer" column="number_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Parmi des mots prédéfinis - voir thesaurus

</option>
      </options>
    </field>
    <field name="beginTc" type="integer" column="begin_tc" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="endTc" type="integer" column="end_tc" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="length" type="integer" column="length" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Calcul de la durée à partir de end_tc - begin_tc</option>
      </options>
    </field>
    <field name="begin" type="string" column="begin" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ending" type="string" column="ending" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="spectators" type="string" column="spectators" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Pondération de l’importance du statut réel ou imaginaire du lieu.
Si 0 : les deux (real et fiction) sont à égalité
Si 1 : real prime
Si 2 : fiction prime</option>
      </options>
    </field>
    <field name="musician" type="string" column="musician" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Une seule réponse possible par liste de mots (en cours de définition) / select</option>
      </options>
    </field>
    <field name="lyrics" type="text" column="lyrics" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Texte des lyrics qui ont été censurés</option>
      </options>
    </field>
    <field name="dance" type="string" column="dance" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Une erreur/ case à ne pas remplir (on la garde au cas où)</option>
      </options>
    </field>
    <field name="tempo" type="string" column="tempo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Voir thesaurus, un seule réponse
Select</option>
      </options>
    </field>
    <field name="makeup" type="string" column="makeUp" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Select voir thesaurus</option>
      </options>
    </field>
    <field name="shots" type="integer" column="shots" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="order" type="integer" column="order" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Calculé à partir des timecode
Calcul par le tc_begin
</option>
      </options>
    </field>
    <field name="integration2" type="string" column="integration2" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">4 catégories (en plus de la table intégration) :
- disconnected
- </option>
      </options>
    </field>
    <field name="weight" type="string" column="weight" length="500" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Lié au lieu

Pondération de l’importance du statut réel ou imaginaire du lieu.
Si 0 : les deux (real et fiction) sont à égalité
Si 1 : real prime
Si 2 : fiction prime</option>
      </options>
    </field>
    <field name="structure" type="string" column="structure" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="diegetic" type="string" column="diegetic" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">OnStage/voir thesaurus (1 réponse parmi  choix)</option>
      </options>
    </field>
    <field name="source" type="string" column="source" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">
1 réponse parmi les 4 :
-original
- broadway
…</option>
      </options>
    </field>
    <many-to-one field="film" target-entity="Film" fetch="LAZY">
      <join-columns>
        <join-column name="film_id" referenced-column-name="film_id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="quotation" target-entity="Quotation" inversed-by="numberNumber" fetch="LAZY">
      <join-table name="number_has_quotation">
        <join-columns>
          <join-column name="number_number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="quotation_id" referenced-column-name="quotation_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="mood" target-entity="Mood" inversed-by="number" fetch="LAZY">
      <join-table name="number_has_mood">
        <join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="mood_id" referenced-column-name="mood_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="song" target-entity="Song" inversed-by="number" fetch="LAZY">
      <join-table name="number_has_song">
        <join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="place" target-entity="Place" mapped-by="numberNumber" fetch="LAZY"/>
    <many-to-many field="stagenumber" target-entity="Stagenumber" inversed-by="number" fetch="LAZY">
      <join-table name="stagenumber_has_number">
        <join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="stageNumber_id" referenced-column-name="stageNumber_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="socialplace" target-entity="Socialplace" inversed-by="numberNumber" fetch="LAZY">
      <join-table name="socialplace_has_number">
        <join-columns>
          <join-column name="number_number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="socialPlace_id" referenced-column-name="socialPlace_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="exoticism" target-entity="Exoticism" inversed-by="numberNumber" fetch="LAZY">
      <join-table name="number_has_exoticism">
        <join-columns>
          <join-column name="number_number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="exoticism_id" referenced-column-name="exoticism_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="ensemble" target-entity="Ensemble" mapped-by="number" fetch="LAZY"/>
    <many-to-many field="costumes" target-entity="Costumes" mapped-by="number" fetch="LAZY"/>
    <many-to-many field="completeness" target-entity="Completeness" mapped-by="number" fetch="LAZY"/>
    <many-to-many field="dancing" target-entity="Dancing" mapped-by="number" fetch="LAZY"/>
    <many-to-many field="integration" target-entity="Integration" mapped-by="number" fetch="LAZY"/>
    <many-to-many field="musical" target-entity="Musical" mapped-by="number" fetch="LAZY"/>
    <many-to-many field="effects" target-entity="Effects" inversed-by="number" fetch="LAZY">
      <join-table name="number_has_effects">
        <join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="effects_id" referenced-column-name="effects_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
