<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Dancing" table="dancing">
    <id name="dancingId" type="integer" column="dancing_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Style of dancing/ Dance sub-genre/ Content</option>
      </options>
    </field>
    <field name="mandatory" type="string" column="mandatory" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="stagenumber" target-entity="Stagenumber" inversed-by="dancing" fetch="LAZY">
      <join-table name="dancing_has_stagenumber">
        <join-columns>
          <join-column name="dancing_id" referenced-column-name="dancing_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="stageNumber_id" referenced-column-name="stageNumber_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="number" target-entity="Number" inversed-by="dancing" fetch="LAZY">
      <join-table name="dancing_has_number">
        <join-columns>
          <join-column name="dancing_id" referenced-column-name="dancing_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="tv" target-entity="Tv" inversed-by="dancing" fetch="LAZY">
      <join-table name="tv_has_dancing">
        <join-columns>
          <join-column name="dancing_id" referenced-column-name="dancing_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="TV_id" referenced-column-name="TV_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
