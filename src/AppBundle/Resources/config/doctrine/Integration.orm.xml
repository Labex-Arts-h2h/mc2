<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Integration" table="integration">
    <id name="integrationId" type="integer" column="integration_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="number" target-entity="Number" inversed-by="integration" fetch="LAZY">
      <join-table name="integration_has_number">
        <join-columns>
          <join-column name="integration_id" referenced-column-name="integration_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="number_id" referenced-column-name="number_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
