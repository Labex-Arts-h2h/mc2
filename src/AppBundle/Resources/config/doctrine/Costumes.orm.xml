<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Costumes" table="costumes">
    <id name="costumesId" type="integer" column="costumes_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Le nom du costume est tir√© de la liste costume / select</option>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="stagenumber" target-entity="Stagenumber" inversed-by="costumes" fetch="LAZY">
      <join-table name="stagenumber_has_costumes">
        <join-columns>
          <join-column name="costumes_id" referenced-column-name="costumes_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="stageNumber_id" referenced-column-name="stageNumber_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
