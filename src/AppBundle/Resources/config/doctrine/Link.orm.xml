<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Link" table="link">
    <indexes>
      <index name="fk_link_number1_idx" columns="number_id"/>
      <index name="fk_link_TV1_idx" columns="TV_id"/>
      <index name="fk_link_disc1_idx" columns="disc_id"/>
      <index name="fk_link_radio1_idx" columns="radio_id"/>
      <index name="fk_link_stageNumber1_idx" columns="stageNumber_id"/>
      <index name="fk_link_stageShow1_idx" columns="stageShow_id"/>
    </indexes>
    <id name="linkId" type="integer" column="link_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="stageshow" target-entity="Stageshow" fetch="LAZY">
      <join-columns>
        <join-column name="stageShow_id" referenced-column-name="stageShow_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tv" target-entity="Tv" fetch="LAZY">
      <join-columns>
        <join-column name="TV_id" referenced-column-name="TV_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="stagenumber" target-entity="Stagenumber" fetch="LAZY">
      <join-columns>
        <join-column name="stageNumber_id" referenced-column-name="stageNumber_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="radio" target-entity="Radio" fetch="LAZY">
      <join-columns>
        <join-column name="radio_id" referenced-column-name="radio_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="number" target-entity="Number" fetch="LAZY">
      <join-columns>
        <join-column name="number_id" referenced-column-name="number_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="disc" target-entity="Disc" fetch="LAZY">
      <join-columns>
        <join-column name="disc_id" referenced-column-name="disc_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
