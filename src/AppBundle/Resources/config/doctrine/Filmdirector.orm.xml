<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Filmdirector" table="filmDirector">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="film" type="string" column="film" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imdb" type="string" column="imdb" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="personId" type="integer" column="person_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="filmId" type="integer" column="film_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
