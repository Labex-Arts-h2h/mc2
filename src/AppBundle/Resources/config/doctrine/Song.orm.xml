<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Song" table="song">
    <id name="songId" type="integer" column="song_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="string" column="date" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="radio" target-entity="Radio" inversed-by="song" fetch="LAZY">
      <join-table name="song_has_radio">
        <join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="radio_id" referenced-column-name="radio_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="stagenumber" target-entity="Stagenumber" inversed-by="song" fetch="LAZY">
      <join-table name="song_has_stagenumber">
        <join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="stageNumber_id" referenced-column-name="stageNumber_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="quotation" target-entity="Quotation" inversed-by="song" fetch="LAZY">
      <join-table name="song_has_quotation">
        <join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="quotation_id" referenced-column-name="quotation_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="disc" target-entity="Disc" inversed-by="song" fetch="LAZY">
      <join-table name="song_has_disc">
        <join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="disc_id" referenced-column-name="disc_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="songtype" target-entity="Songtype" inversed-by="song" fetch="LAZY">
      <join-table name="songtype_has_song">
        <join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="songType_id" referenced-column-name="songType_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="tv" target-entity="Tv" inversed-by="song" fetch="LAZY">
      <join-table name="song_has_tv">
        <join-columns>
          <join-column name="song_id" referenced-column-name="song_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="TV_id" referenced-column-name="TV_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="number" target-entity="Number" mapped-by="song" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
