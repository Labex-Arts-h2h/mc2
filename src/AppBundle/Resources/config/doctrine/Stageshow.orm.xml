<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Stageshow" table="stageShow">
    <indexes>
      <index name="fk_stageShow_film1_idx" columns="film_id"/>
    </indexes>
    <id name="stageshowId" type="integer" column="stageShow_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="production" type="string" column="production" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="opening" type="date" column="opening" nullable="true"/>
    <field name="revival" type="string" column="revival" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Yes / No</option>
      </options>
    </field>
    <field name="ibdb" type="integer" column="ibdb" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="race" type="string" column="race" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="closing" type="date" column="closing" nullable="true"/>
    <field name="count" type="integer" column="count" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Le nombre de dates
</option>
      </options>
    </field>
    <many-to-one field="film" target-entity="Film" fetch="LAZY">
      <join-columns>
        <join-column name="film_id" referenced-column-name="film_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
